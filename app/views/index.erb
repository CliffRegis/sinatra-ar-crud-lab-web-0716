<h1>Listing Posts</h1>

<table>
  <thead>
    <tr>
      <th>Content</th>
      <th>User</th>
      <th colspan="3"></th>
    </tr>
  </thead>

  <tbody>
    <% @posts.each do |str| %>
      <tr>
        <td><%= str.name %></td>
        <td><%= str.content %></td>

      </tr>
    <% end %>
  </tbody>
</table>


